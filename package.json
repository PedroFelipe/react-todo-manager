{
  "name": "react-todo-manager",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "bootstrap": "^3.3.7",
    "express": "^4.16.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.14",
    "redux": "^3.7.2",
    "redux-axios-middleware": "^4.0.0",
    "redux-logger": "^3.0.6",
    "travix-ui-kit": "^0.6.32"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js start-server",
    "start-js": "react-scripts start",
    "start-server": "node app/app.js",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "test:ui": "react-scripts test --env=jsdom",
    "test:app": "mocha -R spec app/spec.js --reporter nyan",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.8.0",
    "eslint-config-travix": "^3.0.1",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^4.0.1",
    "supertest": "^3.0.0"
  },
  "eslintConfig": {
    "extends": [
      "travix/base",
      "travix/react"
    ]
  }
}
